{
  "manifest_version": 3,
  "name": "Website Load Time Tracker",
  "version": "1.0",
  "description": "Track website load times with AI-powered analysis",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}